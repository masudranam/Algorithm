//BigMod
int pw(int a, int b,int M){
    int res = 1;
    while(b){
        if(b&1) res = (res*a)%M;
        a = (a*a) % M;
        b >>= 1;
    }
    return res;
}

//Multiplication with mod
int mul(int a, int b, int M){
    int res = 0;
    while(b){
        if(b&1){
            res = (res + a)%M;
        }
        a = (a+a)%M;
        b >>= 1;
    }
    return res;
}










